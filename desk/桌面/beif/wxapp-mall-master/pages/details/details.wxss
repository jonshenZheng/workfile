.footer-box .cartNum{
    position: absolute;
    right: 20rpx;
    top: 6rpx;
    font-size: 24rpx;
    padding:2rpx 10rpx;
    border-radius: 50%;
    color: #fff;
    background-color: #e73c39;
    line-height: normal;
}

.footer-box .showAnimate{
    position: absolute;
    right: 8rpx;
    top: -24rpx;
    font-weight: bold;
    font-size: 26rpx;
    color: #e73c39;
    line-height: normal;
    visibility: hidden;
}

.footer-box .showAnimate.on{
    animation: showcartNum 1s forwards; 
    -webkit-animation :showcartNum 1s forwards; 
}

@-webkit-keyframes showcartNum{
    0%{
        visibility: visible;
        transform: translateY(0);
        opacity: 1;
    }
    80%{
        transform: translateY(-40rpx);
        opacity: 9;
    }
    100%{
        transform: translateY(-40rpx);
        opacity: 0;
    }
}

@keyframes showcartNum{
    0%{
        visibility: visible;
        transform: translateY(0);
        opacity: 1;
    }
    80%{
        transform: translateY(-40rpx);
        opacity: 9;
    }
    100%{
        transform: translateY(-40rpx);
        opacity: 0;
        visibility: hidden;
    }
}


