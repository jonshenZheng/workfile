.qiuckNavigMask{
    position: fixed;
    top:0;
    left:0;
    width: 100%;
    height: 100%;
    background-color: #000;
    opacity: .1;
    display: none;
}

.qiuckNavigMask.active{
    display: block;
}

.qiuckNavig{
    position: fixed;
    right: 0;
    bottom: 200rpx;
    width: 300rpx;
    clear: both;
    transform: translateX(196rpx);
    -webkit-transform: translateX(196rpx);
    transition: transform .5s;
    -webkit-transition: transform .5s;
}

.qiuckNavig.active{
    transform: translateX(56rpx);
    -webkit-transform: translateX(56rpx);
}

.qiuckNavig .iconBox{
    float: left;
    width: 98rpx;
    height: 64rpx;
    position: relative;
    padding: 8rpx 0 8rpx 8rpx;
}

.qiuckNavig .iconBox .bg{
    position: absolute;
    width: 100%;
    height: 100%;
    background-color:#000;
    opacity: .8;
    top:0;
    left:0;
}

.qiuckNavig .iconBox .qbtn{
    display: inline-block;
    width: 24rpx;
    height: 24rpx;
    background: url(https://baize-webresource.oss-cn-shenzhen.aliyuncs.com/weixin/image/ic_navLabel_open.png);
    background-size: 100% 100%;
    margin-right: 6rpx;
}

.qiuckNavig .iconBox .qbtn.active{
    background: url(https://baize-webresource.oss-cn-shenzhen.aliyuncs.com/weixin/image/ic_navLabel_back.png);
    background-size: 100% 100%;
}

.qiuckNavig .iconBox .centen text{
    font-size: 22rpx;
    color: #fff;
    width: 50rpx;
    display: inline-block;
    height: 60rpx;
}

.qiuckNavig .iconBox .centen text.active{
    line-height: 60rpx;
}

.qiuckNavig .iconBox .centen{
    position: relative;
}

.qiuckNavig .iconBox .centen text,.qiuckNavig .iconBox .qbtn{
    vertical-align: middle;
}

.qiuckNavig .qBtnBox{
    float: left;
