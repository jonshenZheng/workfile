.showAnimate{
    position: absolute;
    left: 50%;
    bottom: 1%;
    width: 10%;
    margin-left: -5%;
    padding-top: 5.56%;
    background: url(https://baize-webresource.oss-cn-shenzhen.aliyuncs.com/weixin/image/showMorePage.png);
    background-size: 100% 100%;
    animation: slideUp 1.6s infinite;
}

@keyframes slideUp{
    0%{
        transform: translateY(0);
        opacity: 1;
    }

    25%{
        opacity: 1;
    }

    50%{
        opacity: 1;
    }

    100%{
        transform: translateY(-160%);
        opacity: 0;
    }

}


