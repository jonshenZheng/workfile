.newest-box{
    position: relative;
    overflow: hidden;
    margin-bottom: 20rpx;
    display: flex;
    flex-flow: row wrap;
}
.newest-box .newest-list{
    position: relative;
    display: inline-block;
    width: 50%;
    height: 460rpx;
    box-sizing: border-box;
    border-right: 3px solid #eee;
    border-bottom: 3px solid #eee;
    padding: 0 16rpx;
}
.newest-box .newest-text.pdName{
    color: #666;
    padding-bottom: 6rpx;
    text-overflow: ellipsis;
    white-space: nowrap;
    overflow: hidden;
}

.newest-box .newest-text{
    color: #ccc;
}

.newest-box .newest-text .type{
    display: block;
    white-space: nowrap;
    text-overflow: ellipsis;
    overflow: hidden;
}

.newest-box .newest-list:nth-child(2n){
    margin-right: 0;
    border-right: 0;
}

.newest-box .newest-list image{
    width: 344rpx;
    height: 344rpx;
    margin: 0rpx 0 10rpx;
}
.newest-box .newest-list .newest-text{
    font-size: 28rpx;
}

