    display: block;
    width: 100%;
    height: auto;
    max-height: 236.66292rpx;
}

.showAnimate{
    position: absolute;
    left: 50%;
    bottom: 1%;
    width: 10%;
    margin-left: -5%;
    padding-top: 5.56%;
    background: url(http://localhost/image/showMorePage.png);
    background-size: 100% 100%;
    animation: slideUp 1.6s infinite;
}

@keyframes slideUp{
    0%{
        transform: translateY(0);
        opacity: 1;
    }

    25%{
        opacity: 1;
    }

    50%{
        opacity: 1;
    }

    100%{
        transform: translateY(-160%);
        opacity: 0;
    }

}


