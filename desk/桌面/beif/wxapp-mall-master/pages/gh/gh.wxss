/* pages/test/test.wxss */

page{
    background-color: #f7f7f7;
}

.mian{
    height: 100%;
    display: flex;
    flex-direction: column;
}

.headTab{
    display: flex;
    background-color: #fff;
    width: 100%;
    height: 98rpx;
    padding-bottom: 2px;
    box-sizing: content-box;
}

.tableScroll{
    height: 100%;
    flex:1;
    flex-shrink:1;
    flex-grow:1;
}


.headTab .btn_a{
    flex:1;
    text-align: center;
}

.headTab .btn_a .na{
    display: inline-block;
    padding: 0 12rpx;
    height: 98rpx;
    line-height: 98rpx;
    border-bottom: 2px solid #fff;
}

.headTab .btn_a.active .na{
    color: #1890FF;
    border-color: #1890FF;
}

.pageV1 .topBox{
    padding: 10rpx 0;
}

.pageV1 .topBox .sereachBox{
    margin: 0 20rpx 10rpx;
}

.pageV1 .topBox .sereachBox{
    height: 48rpx;
    line-height: 48rpx;
    border-radius: 10rpx;
    text-align: center;
    background: url(https://baize-webresource.oss-cn-shenzhen.aliyuncs.com/weixin/image/xiaoshouXS-sereach.png) center center no-repeat #fff;
    background-size: 92rpx 24rpx;
}

.pageV1 .topBox .updateText{
    text-align: center;
    height: 70rpx;
    line-height: 70rpx;
    color: #f60;
    font-size: 24rpx;
    background-color:#FEFCEC;
}

.xiansuoBox .it{
    background-color: #fff;
    margin-bottom: 10rpx;
}

.xiansuoBox .it .tp{
    padding: 26rpx;
}
.xiansuoBox .it .ifo{
    padding-bottom: 10rpx;
}

.xiansuoBox .it .name,.xiansuoBox .it .phone{
    font-size: 32rpx;
}

.xiansuoBox .it .name,.xiansuoBox .it .phone,.xiansuoBox .it .status{
    display: inline-block;
    margin-right: 24rpx;
}

.xiansuoBox .it .status{
    padding: 4rpx 10rpx;
    background-color: #BCBCBC;
    font-size:24rpx;
    border-radius: 6rpx;
    color:#fff;
}

.xiansuoBox .it .status.on{
    background-color: #1890ff;
    border-radius: 20rpx;
}

.xiansuoBox .locati{
    display: flex;
}

.xiansuoBox .locati .la{
    font-size: 28rpx;
    color: #888;
}

.xiansuoBox .locati .la1,.xiansuoBox .locati .la2{
    width: 50%;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space : nowrap;
}

.xiansuoBox .locati .la3{
    width: 246rpx;
}

.xiansuoBox .locati icon{
    margin:0 6rpx -4rpx 0;
}

.xiansuoBox .locati .ico-time{
    display: inline-block;
    width: 28rpx;
    height: 28rpx;
    background: url(https://baize-webresource.oss-cn-shenzhen.aliyuncs.com/weixin/image/xiaoshouXS-time.png);
    background-size: 100% 100%;
}

.xiansuoBox .locati .ico-location{
    display: inline-block;
    width: 24rpx;
    height: 28rpx;
    background: url(https://baize-webresource.oss-cn-shenzhen.aliyuncs.com/weixin/image/xiaoshouXS-location.png);
    background-size: 100% 100%;
}

.xiansuoBox .locati .ico-home{
    display: inline-block;
    width: 32rpx;
    height: 28rpx;
    background: url(https://baize-webresource.oss-cn-shenzhen.aliyuncs.com/weixin/image/xiaoshouXS-home.png);
    background-size: 100% 100%;
}

.xiansuoBox .bt .bt1,.xiansuoBox .bt .bt2{
    float: left;
    width: 50%;
    height: 80rpx;
    line-height: 80rpx;
    text-align: center;
    font-size: 28rpx;
}

.xiansuoBox .bt .ico-key{
    display: inline-block;
    width: 22rpx;
    height: 40rpx;
    background: url(https://baize-webresource.oss-cn-shenzhen.aliyuncs.com/weixin/image/xiaoshouXS-key.png);
    background-size: 100% 100%;
    margin-bottom: -10rpx;
    margin-right: 20rpx;
}

.xiansuoBox .bt .ico-heart{
    display: inline-block;
    width: 36rpx;
    height: 36rpx;
    background: url(https://baize-webresource.oss-cn-shenzhen.aliyuncs.com/weixin/image/xiaoshouXS-hart.png);
    background-size: 100% 100%;
    margin-bottom: -8rpx;
    margin-right: 20rpx;
}

.xiansuoBox .bt .ico-heart.on{
    background: url(https://baize-webresource.oss-cn-shenzhen.aliyuncs.com/weixin/image/xiaoshouXS-hart-on.png);
    background-size: 100% 100%;    
}

.xiansuoBox .bt .ico-select{
    display: inline-block;
    width: 36rpx;
    height: 36rpx;
    background: url(https://baize-webresource.oss-cn-shenzhen.aliyuncs.com/weixin/image/xiaoshouXS-select.png);
    background-size: 100% 100%;
    margin-bottom: -8rpx;
    margin-right: 20rpx;
}


.xiansuoBox .bt .ico-lock{
    display: inline-block;
    width: 34rpx;
    height: 36rpx;
    background: url(https://baize-webresource.oss-cn-shenzhen.aliyuncs.com/weixin/image/xiaoshouXS-lock.png);
    background-size: 100% 100%;
    margin-bottom: -8rpx;
    margin-right: 20rpx;
}



.xiansuoBox .bt .bt1{
    position: relative;
}

.xiansuoBox .bt .bord{
    position: absolute;
    right: 0;
    top: 20%;
    height: 60%;
    width: 1px;
}

.pageV2 .page2Tab{
    margin: 20rpx 30rpx;
}

.pageV2 .page2Tab .wp1,.pageV2 .page2Tab .wp2{
    width: 300rpx;
    height: 54rpx;
    line-height: 54rpx;
    text-align: center;
    font-size: 30rpx;
    border-radius: 75rpx;
    background-color: #fff;
    color: #999;
}

.pageV2 .page2Tab .wp1.active,.pageV2 .page2Tab .wp2.active{
    background-color: #1890ff;
    color: #fff;
}
    

.pageV2 .page2Tab .wp1{
    float: left;
}
.pageV2 .page2Tab .wp2{
    float: right;
}

.pageV2 .xiansuoBox .ico-telephone{
    display: inline-block;
    width: 36rpx;
    height: 36rpx;
    background: url(https://baize-webresource.oss-cn-shenzhen.aliyuncs.com/weixin/image/xiaoshouXS-telephone.png);
    background-size: 100% 100%;
    margin-right: 20rpx;
    margin-bottom: -8rpx;
}

.pageV2 .xiansuoBox .ico-remove{
    display: inline-block;
    width: 42rpx;
    height: 40rpx;
    background: url(https://baize-webresource.oss-cn-shenzhen.aliyuncs.com/weixin/image/xiaoshouXS-remove.png);
    background-size: 100% 100%;
    margin-right: 20rpx;
    margin-bottom: -10rpx;
}


.maskBox{
    display: none;
    position: fixed;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    background-color: #afafaf;
    z-index: 2;
}

.upMuneBox{
    position: fixed;
    bottom: 0;
    left: 0;
    width: 100%;
    transform: translateY(100%);
    transition: all .3s;
    background-color: #e5e5e9;
    z-index: 2;
}

.upMuneBox .listBox{
    background-color: #fff;
    margin-bottom: 20rpx;
}

.upMuneBox .listBox .it,.upMuneBox .cancleBtn{
    height: 100rpx;
    line-height: 100rpx;
    text-align: center;
    font-size: 32rpx;
    color: #000;
}

.upMuneBox .cancleBtn{
    background-color: #fff;
}

.maskBox.active{
    display: block;
}

.upMuneBox.active{
    transform: translateY(0);
}


































@import '../common/common.wxss';