/* pages/test/test.wxss */

page{
    background-color: #f7f7f7;
    height: auto;
}

.mian{
    padding-bottom: 104rpx;
}

.timeBox{
    padding: 30rpx 20rpx 10rpx;
    position: relative;
}

.timeBox .timeTips{
    font-size: 32rpx;
    color: #1E1E1E;
    padding-bottom: 10rpx;
    display: block;
}

.timeBox .timeW{
    padding-top: 10rpx;
    font-size: 40rpx;
    color: #F43530;
}

.timeBox .tipsDel{
    font-size: 26rpx;
    color: #AEAEAE;
    margin-right:142rpx;
}

.timeBox .tBTN{
    position: absolute;
    right:20rpx;
    top: 50%;
    margin-top: -30rpx;
    width: 140rpx;
    height: 60rpx;
    line-height: 60rpx;
    font-size: 26rpx;
    color: #fff;
    border-radius:6rpx;
    background-color:#489AF7; 
}

.bgBox{
    margin-top: 20rpx;
    padding: 30rpx 20rpx;
    background-color: #fff;
}

.headInfo .nameBox{
    padding-bottom: 20rpx;
}

.headInfo .nameBox .fl{
    font-size: 32rpx;
    color: #000;
}

.headInfo .nameBox .fr{
    font-size: 28rpx;
    color: #A1A1A1;
}

.headInfo .nameBox .fr:first-child{
    margin-left: 24rpx;
}

.headInfo .nameBox .type{
    padding: 4rpx 8rpx;
    background-color: #1890FF;
    color:#fff;
    border-radius: 30rpx;
    font-size: 26rpx;
}
 
.headInfo .phoneBox{
    padding-bottom: 20rpx;
}

.headInfo .phoneBox{
    /* padding-left: 20rpx; */
}

.headInfo .phoneBox .fl{
    font-size: 32rpx;
    color: #000;
}

.headInfo .phoneBox .fr{
    font-size: 24rpx;
    color: #fff;
    padding: 4rpx 8rpx;
    background-color: #bcbcbc;
    border-radius: 6rpx;
}

.headInfo .tipsBox{
    font-size: 28rpx;
    color: #489af7;
}

.saleInfoBox{
    padding-bottom: 0;
    padding-top: 0;
}

.saleInfoBox .it{
    padding: 24rpx 0;
}

.saleInfoBox .it:last-child::after{
    display: none;
}

.saleInfoBox .it .til{
    font-size: 32rpx;
    color: #000;
}
.saleInfoBox .it .val{
    font-size: 32rpx;
    color: #A1A1A1;
}

.genzhongjilv-tipBox{
    padding: 30rpx 20rpx;
}

.genzhongjilv-tipBox .til{
    font-size: 32rpx;
    color: #1E1E1E;
}

.genzhongjilv-tipBox .val{
    font-size: 26rpx;
    color: #AEAEAE;
}

.genzhongjilv-pane{
    background-color: #fff;
    padding: 0 0 10rpx 20rpx;
    margin-bottom: 20rpx;
}

.genzhongjilv-pane .til{
    position: relative;
}

.genzhongjilv-pane .til text{
    font-size: 32rpx;
    color: #1E1E1E;
    display: block;
    height: 88rpx;
    line-height: 88rpx;
    margin-right: 60rpx;
}

.genzhongjilv-pane .ico-remove{
    position: absolute;
    width: 60rpx;
    height: 60rpx;
    /* width: 38rpx;
    height: 36rpx; */
    background: url(https://baize-webresource.oss-cn-shenzhen.aliyuncs.com/weixin/image/mySalesInfo-remove2.png) center center no-repeat;
    background-size: 63.33% 60%;
    right: 20rpx;
    top: 50%;
    margin-top: -30rpx;
}

.genzhongjilv-pane .detail{
    padding: 20rpx 20rpx 20rpx 0;
    text-align: justify;
    font-size: 30rpx;
    color: #999999;
}

.genzhongjilv-pane .jl-time{
    padding-right: 20rpx;
}

.genzhongjilv-pane .jl-time .fl,.genzhongjilv-pane .jl-time .fr{
    font-size: 28rpx;
    color: #999;
}







.footerBtn{
    position: fixed;
    bottom: 0;
    left: 0;
    width: 100%;
    z-index: 1;
}

.footerBtn .btn{
    float: left;
    width: 50%;
    text-align: center;
    height: 84rpx;
    line-height: 84rpx;
    font-size: 32rpx;
    color: #fff;
    background-color: #1890FF;
    position: relative;
}

.footerBtn .btn .bord{
    position: absolute;
    right: 0;
    top: 0;
    height: 100%;
    width: 1px;
}

.footerBtn .btn .bord::after{
    border-color: #fff !important;
}

.footerBtn .btn .ico-record{
    display: inline-block;
    width: 48rpx;
    height: 48rpx;
    background: url(https://baize-webresource.oss-cn-shenzhen.aliyuncs.com/weixin/image/mySalesInfo-record.png);
    background-size: 100% 100%;
    margin-right: 20rpx;
    margin-bottom: -12rpx;
}

.footerBtn .btn .ico-telephone2{
    display: inline-block;
    width: 40rpx;
    height: 40rpx;
    background: url(https://baize-webresource.oss-cn-shenzhen.aliyuncs.com/weixin/image/mySalesInfo-telephone2.png);
    background-size: 100% 100%;
    margin-right: 20rpx;
    margin-bottom: -8rpx;
}

.footerBtn .btn .ico-remove{
    display: inline-block;
    width: 48rpx;
    height: 48rpx;
    background: url(https://baize-webresource.oss-cn-shenzhen.aliyuncs.com/weixin/image/mySalesInfo-remove.png);
    background-size: 100% 100%;
    margin-right: 20rpx;
    margin-bottom: -12rpx;
}


.footerBtn .btn .ico-key{
    display: inline-block;
    width: 40rpx;
    height: 40rpx;
    background: url(https://baize-webresource.oss-cn-shenzhen.aliyuncs.com/weixin/image/publicSalesInfo-key.png);
    background-size: 100% 100%;
    margin-right: 20rpx;
    margin-bottom: -8rpx;
}

.footerBtn .btn .ico-heart{
    display: inline-block;
    width: 40rpx;
    height: 40rpx;
    background: url(https://baize-webresource.oss-cn-shenzhen.aliyuncs.com/weixin/image/publicSalesInfo-heart.png);
    background-size: 100% 100%;
    margin-right: 20rpx;
    margin-bottom: -8rpx;
}

.footerBtn .btn .ico-heart.on{
    background: url(https://baize-webresource.oss-cn-shenzhen.aliyuncs.com/weixin/image/publicSalesInfo-heart-on.png);
    background-size: 100% 100%;
}

.footerBtn .btn .ico-lock{
    display: inline-block;
    width: 40rpx;
    height: 40rpx;
    background: url(https://baize-webresource.oss-cn-shenzhen.aliyuncs.com/weixin/image/publicSalesInfo-lock.png);
    background-size: 100% 100%;
    margin-right: 20rpx;
    margin-bottom: -8rpx;
}



.companyName{
    font-size: 32rpx;
    color: #000;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
} 

.headInfo .phoneBox .callBtn{
    padding: 0;
    background-color: transparent;
}

.headInfo .phoneBox .callBtn text{
    font-size: 30rpx;
    color: #489AF7;
}

.headInfo .phoneBox .ico-telephone{
    display: inline-block;
    width: 30rpx;
    height: 30rpx;
    background: url(https://baize-webresource.oss-cn-shenzhen.aliyuncs.com/weixin/image/mySalesInfo-telephone.png);
    background-size: 100% 100%;
    margin-right: 20rpx;
    margin-bottom: -4rpx;
}

.infoStatus .til{
    font-size: 32rpx;
    color: #000;
}

.infoStatus .val{
    font-size: 32rpx;
    color: #A1A1A1;
}

.infoStatus .ico-tri{
    display: inline-block;
    width: 16rpx;
    height: 26rpx;
    background: url(https://baize-webresource.oss-cn-shenzhen.aliyuncs.com/weixin/image/mySalesInfo-tri.png);
    background-size: 100% 100%;
    margin-left: 10rpx;
    margin-bottom: -2rpx;
}



.maskBox{
    display: none;
    position: fixed;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    background-color: #afafaf;
    z-index: 2;
}

.upMuneBox{
    position: fixed;
    bottom: 0;
    left: 0;
    width: 100%;
    transform: translateY(100%);
    transition: all .3s;
    background-color: #e5e5e9;
    z-index: 2;
}

.upMuneBox .listBox{
    background-color: #fff;
    margin-bottom: 20rpx;
}

.upMuneBox .listBox .it,.upMuneBox .cancleBtn{
    height: 100rpx;
    line-height: 100rpx;
    text-align: center;
    font-size: 32rpx;
    color: #000;
}

.upMuneBox .cancleBtn{
    background-color: #fff;
}

.maskBox.active{
    display: block;
}

.upMuneBox.active{
    transform: translateY(0);
}




@import '../../common/common.wxss';