.vipBox{
    margin: 22rpx 17px;
    border: 1px solid #d6c5c7;
    padding: 26rpx 0 16rpx 0;
    background-color: #fbf3ea;
    min-height: 150rpx;
    position: relative;
    border-radius:6px;
}

.vipBox .medal{
    position: absolute;
    left: 20rpx;
    top: 0;
    height: 180rpx;
    width: 122rpx;
    background: url('https://baize-webresource.oss-cn-shenzhen.aliyuncs.com/weixin/image/user-medal.png');
    background-size: 100% 100%;
}

.vipBox .medal.shiyong{
    background: url('https://baize-webresource.oss-cn-shenzhen.aliyuncs.com/weixin/image/user-medal-shiyong.png');
    background-size: 100% 100%;
}

.vipBox .til{
    font-size: 34rpx;
    font-weight: bold;
    padding-bottom: 26rpx;
}
.vipBox .dec{
    font-size: 26rpx;
    text-align: justify;
    line-height: 32rpx;
    padding-right: 32rpx;
}

.vipBox .til,.vipBox .dec{
    padding-left: 156rpx;
}

.vipBtnBox{
    padding: 40rpx 16rpx 0 16rpx;
    position: relative;
}

.headVipWp .vipBtnBox{
    position: absolute;
    bottom: 5%;
    width: 100%;
    box-sizing: border-box;
    left: 0;
}

.vipBtnBox .timeTips{
    position: absolute;
    left: 16rpx;
    top: 0rpx;
    color: #e96e71;
    font-size: 18rpx;
}

.vipBtnBox .timeBox{
    float: left;
}

.vipBtnBox .timeBox .tBox,.vipBtnBox .timeBox .unit{
    float: left;
}

.vipBtnBox .timeBox .tBox{
    font-size: 32rpx;
    color: #000;
    border-radius: 5px;
    border: 1px solid #cfad7e;
    width: 50rpx;
    text-align: center;
    height: 60rpx;
    line-height: 60rpx;
}

.vipBtnBox .timeBox .unit{
    padding: 28rpx 12rpx 0 8rpx;
    box-sizing: content-box;
    font-size: 22rpx;
}

.vipBtnBox .btnV,.headVipWp .btnV{
    float: right;
    width: 260rpx;
    height: 60rpx;
    text-align: center;
    line-height: 60rpx;
    background: url('https://baize-webresource.oss-cn-shenzhen.aliyuncs.com/weixin/image/user-regist-bg.png');
    background-size: 100% 100%;
    font-size: 26rpx;
    color: #fff;
}

.vipBtnBox .btnB,.headVipWp .btnB{
    display: inline-block;
    width: 26rpx;
    height: 30rpx;
    background: url('https://baize-webresource.oss-cn-shenzhen.aliyuncs.com/weixin/image/user-wallet.png');
    background-size: 100% 100%;
    margin-top: 10rpx;
    position: relative;
    top: 4rpx;
    margin-right: 6rpx;
}

.vipBox.timeOver .timeTips,.vipBox.timeOver .timeBox{
    display: none;
}

.vipBox.timeOver .btnV{
    float: none;
    display: block;
    margin: 0 auto;
}

.vipBox.realVip .vipBtnBox{
    display: none;
}


