.btn-on-off{
    width: 74rpx;
    height: 44rpx;
    border-radius: 25px;
    background-color: #ccc;
    position: relative;
    -webkit-transition: 0.4s;
    transition: 0.4s;
}

.btn-on-off icon{
    position: absolute;
    left: 0;
    top: 0;
    width: 44rpx;
    height: 44rpx;
    border-radius: 50%;
    box-shadow: 0 0 4rpx rgba(0, 0, 0, 0.2);
    background-color: #fff;
    -webkit-transition: 0.4s;
    transition: 0.4s;
}

.btn-on-off.active{
    background-color: #4BD865;
}

.btn-on-off.active icon{
    transform: translateX(30rpx);
    -webkit-transform: translateX(30rpx);
}

