.bindPhone{
    position: fixed;
    width: 100%;
    height: 100%;
    left: 0;
    top: 0;
    z-index: 3;
}

.bindPhone .bg{
    position: absolute;
    width: 100%;
    height: 100%;
    left: 0;
    top: 0;
    opacity: 0.5;
    background-color: #000;
}

.bindPhone .cent{
    text-align: center;
    padding: 10rpx 0;
    position: absolute;
    left: 50%;
    top:50%;
    transform: translate(-50%,-50%);
    -webkit-transform: translate(-50%,-50%);
    width: 80%;
    background-color: #fff;
    border-radius: 10rpx;
}

.bindPhone .til{
    font-size:40rpx;
    padding:30rpx 0;
}

.bindPhone .btnbx{
    padding-top: 16rpx;
    padding-bottom:10rpx;
}

.bindPhone .btnbx text,.bindPhone .btnbx button{
    width:128rpx;
    height:64rpx;
    line-height:64rpx;
    border-radius:6rpx;
    margin:0 40rpx;
    display:inline-block;
    border:0;
    vertical-align:middle;
    font-size:32rpx;
}

.bindPhone .btnbx .cancle{
    background-color:#ccc;
    color:#999;
}

