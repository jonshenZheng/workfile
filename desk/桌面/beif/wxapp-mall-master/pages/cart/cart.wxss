@import '../common/common.wxss';

.cart-box{
    padding-bottom: 100rpx;
}
.cart-list{
    position: relative;
    padding: 20rpx 20rpx 20rpx 285rpx;
    height: 185rpx;
    border-bottom: 1rpx solid #e9e9e9;
}
.cart-list .cart-pro-select{
    position: absolute;
    left: 20rpx;
    top: 90rpx;
    width: 45rpx;
    height: 45rpx;
}

.cart-list .cart-thumb{
    position: absolute;
    top: 20rpx;
    left: 85rpx;
    width: 185rpx;
    height: 185rpx;
}
.cart-list .cart-pro-name{
    display: inline-block;
    width: 300rpx;
    height: 105rpx;
    line-height: 50rpx;
    overflow: hidden;
}
.cart-list .cart-pro-price{
    display: inline-block;
    float: right;
    height: 105rpx;
    line-height: 50rpx;
}
.cart-list .cart-count-box{
    position: absolute;
    left: 285;
    bottom: 20rpx;
    width: 250rpx;
    height: 80rpx;
}
.cart-list .cart-count-box text{
    display: inline-block;
    line-height: 80rpx;
    text-align: center;
}

.cart-count-num{
    width: 150rpx;
}
.cart-del{
    position: absolute;
    right: 20rpx;
    bottom: 20rpx;
    width: 80rpx;
    height: 80rpx;
    line-height: 80rpx;
    text-align: center;
    font-size: 44rpx;
}

.cart-no-data{
    padding:40rpx 0;
    color: #999;
    text-align: center;
}

.cart-footer{
    display: flex;
    justify-content: space-between;
    width: 100%;
    height: 100rpx;
    position: fixed;
    bottom: 0;
    left: 0;
    border-bottom:1px solid #eee; 
    border-top:1px solid #eee; 
    background-color: #fff;
}
.cart-footer .to-pay-btn{
    width:240rpx;
    text-align: center;
    line-height: 100rpx;
    background-color: #e64340;
    font-size:32rpx;
    color:#ffffff;
}
.cart-footer .to-pay-btn.no-select{
    background-color: #ccc;
}

.cart-footer .left-price{
    display: flex;
    width: 510rpx;
    justify-content: space-between;
    line-height: 100rpx;
    padding: 0 30rpx 0 0;
    font-size:28rpx;
    box-sizing: border-box;
}
.cart-footer .all-selected{
    padding-left: 90rpx;
    color:#000000;
    background: url(https://cdn.it120.cc/images/weappshop/gou.png) no-repeat 30rpx center;
    background-size: 40rpx auto; 
}
.cart-footer .all-selected.active{
    background: url(https://cdn.it120.cc/images/weappshop/gou-red.png) no-repeat 30rpx center;
    background-size: 40rpx auto; 
}
.cart-footer .total{
    color: #e64340;
}




page{
    background-color: #f0f2f5;
}
.colBox{
    background-color: #fff;
    margin-bottom: 100rpx;
}
.colBox .cloIt{
    padding: 30rpx 0 50rpx 0;
    position: relative;
    border-bottom: 2rpx solid #ccc;
    overflow: hidden;
}

.colBox .scrollBox{
    width: 200%;
    position: relative;
    transition: transform .4s;
}

.colBox .scrollBox.show{
    transform: translateX(-86rpx);
    -webkit-transform: translateX(-86rpx);
}

@keyframes showDel{
    form{
        transform: translateX(0);
    }
    to{
        transform: translateX(-86rpx);
    }
}

@-webkit-keyframes showDel{
    form{
        -webkit-transform: translateX(0);
    }
    to{
        -webkit-transform: translateX(-86rpx);
    }
}

.colBox .scrollBox .oriWid{
    width: 50%;
    position: relative;
    min-height: 112rpx;
}
.colBox .scrollBox .realdelBtn{
    position: absolute;
    left: 50%;
    top: 0;
    height: 100%;
    background-color: #f00;
    color: #fff;
    font-size:30rpx;
    width: 86rpx;
    text-align: center;
    display: flex;
    justify-content:center;
    align-items:center;
}


.colBox .selBtn{
    position: absolute;
    top: 50%;
    left: 20rpx;
    margin-top: -26rpx;
    width: 36rpx;
    height: 36rpx;
    border: 2rpx solid #797979;
    border-radius:50%;

}

.colBox .selBtn.on,.colBox .selBtn.true{
    /* border-color: #1ba2e8;
    background-color :#1ba2e8; */
    border: 0;
    width: 40rpx;
    height: 40rpx;
    background: url(https://baize-webresource.oss-cn-shenzhen.aliyuncs.com/weixin/image/ic_selectEffect_on.png);
    background-size: 100% 100%;
}

.colBox .cloIt .imgwp{
    position: absolute;
    left : 100rpx;
    top : 0;
    width: 112rpx;
    height: 112rpx;
}

.colBox .cloIt .imgbx{
    width: 112rpx;
    height: 112rpx;
}

.colBox .imgwp .status{
    position: absolute;
    left: 0;
    bottom: 0;
    height: 36rpx;
    width: 100%;
}

.colBox .imgwp .status .bg{
    position: absolute;
    left: 0;
    top: 0;
    height: 100%;
    width: 100%;
    background-color: #000;
    opacity: 0.7;
}
.colBox .imgwp .status text{
    position: relative;
    display: block;
    text-align: center;
    color: #fff;
    font-size: 22rpx;
    line-height: 36rpx;
}

.colBox .cloIt image{
    max-width: 112rpx;
    max-height: 112rpx;
    display: block;
}

.colBox .right-info{
    margin: 0 20rpx 0 230rpx;
    position: relative;
    min-height: 112rpx;
}

.colBox .cloIt .gdName{
    font-size: 30rpx;
    color: #666;
    display: block;
}

.colBox .right-info navigator{
    margin-right: 100rpx;
}

.colBox .cloIt .gdDec{
    font-size: 28rpx;
    min-height: 20rpx;
    display: block;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
    color: #999;
}

.colBox .cloIt .gdPrice{
    font-size: 26rpx;
}

.colBox .cloIt .gdNum{
    font-size: 38rpx;
    color: #666;
    position:absolute;
    right:0;
    top:50%;
    margin-top:-26rpx;

}
.textHide{
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
}

.colBox .cloIt .delBtn{
    position: absolute;
    right: 20rpx;
    top: 0;
    width: 36rpx;
    height: 36rpx;
    background: url(https://baize-webresource.oss-cn-shenzhen.aliyuncs.com/weixin/image/popup-close.png) no-repeat;
    background-size: 100% 100%;
}

.colBox .cart-count-box{
    position:absolute;
    right:70rpx;
    top:86rpx;
}

.colBox .cart-count-box text{
    display: inline-block;
    vertical-align: middle;
}

.colBox .cart-count-down,.colBox .cart-count-add{
    width: 50rpx;
    height: 36rpx;
    line-height: 34rpx;
    text-align: center;
    font-weight:lighter;
    font-size: 44rpx;
    border:2rpx solid #ccc;
}

.colBox .cart-count-num{
    width: 100rpx;
    text-align: center;
}

.colBox .cart-count-down.true,.colBox .cart-count-add.true{
    color: #ccc;
}

